{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "npm run api:codegen",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "api:fetch-docs": "tsx scripts/fetch-openapi-docs.ts",
    "api:generate-types": "openapi-typescript lib/client/openapi.json -o lib/client/types.ts",
    "api:codegen": "npm run api:fetch-docs && npm run api:generate-types"
  },
  "dependencies": {
    "@tanstack/react-query": "5",
    "@tanstack/react-query-devtools": "5",
    "@workspace/ui": "workspace:*",
    "lucide-react": "^0.475.0",
    "next": "15.5.1-canary.6",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "openapi-typescript": "^7.9.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
